{
  "name": "lakutata",
  "version": "0.0.3-alpha.2",
  "description": "An IoC-based universal application framework.",
  "type": "module",
  "exports": {
    ".": {
      "require": "./build/Core.cjs",
      "import": "./build/Core.js",
      "types": "./build/Core.d.ts"
    },
    "./validator": {
      "require": "./build/Validator.cjs",
      "import": "./build/Validator.js",
      "types": "./build/Validator.d.ts"
    },
    "./decorator": {
      "require": "./build/Decorators.cjs",
      "import": "./build/Decorators.js",
      "types": "./build/Decorators.d.ts"
    },
    "./exception": {
      "require": "./build/Exceptions.cjs",
      "import": "./build/Exceptions.js",
      "types": "./build/Exceptions.d.ts"
    },
    "./component": {
      "require": "./build/Components.cjs",
      "import": "./build/Components.js",
      "types": "./build/Components.d.ts"
    },
    "./time": {
      "require": "./build/Time.cjs",
      "import": "./build/Time.js",
      "types": "./build/Time.d.ts"
    },
    "./utility": {
      "require": "./build/Utilities.cjs",
      "import": "./build/Utilities.js",
      "types": "./build/Utilities.d.ts"
    },
    "./hash": {
      "require": "./build/Hash.cjs",
      "import": "./build/Hash.js",
      "types": "./build/Hash.d.ts"
    },
    "./crypto": {
      "require": "./build/Crypto.cjs",
      "import": "./build/Crypto.js",
      "types": "./build/Crypto.d.ts"
    }
  },
  "scripts": {
    "clean": "shx rm -rf ./build",
    "build": "tsup-node --config tsup.config.js",
    "rebuild": "npm run clean && npm run build",
    "prepare": "npm run compile",
    "compile": "npm run rebuild && shx rm -rf ./build/tests/*",
    "release:major": "release-it major --git.tagExclude='*[-]*'",
    "release:major:beta": "release-it major --preRelease=beta",
    "release:major:alpha": "release-it major --preRelease=alpha",
    "release:minor": "release-it minor --git.tagExclude='*[-]*'",
    "release:minor:beta": "release-it minor --preRelease=beta",
    "release:minor:alpha": "release-it minor --preRelease=alpha",
    "release:patch": "release-it patch --git.tagExclude='*[-]*'",
    "release:patch:beta": "release-it patch --preRelease=beta",
    "release:patch:alpha": "release-it patch --preRelease=alpha",
    "release:preRelease": "release-it --preRelease=alpha",
    "test": "ts-node src/tests/Test.spec.ts"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": true,
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    }
  },
  "devDependencies": {
    "@types/convert-units": "^2.3.6",
    "@types/crypto-js": "^4.1.1",
    "@types/is-glob": "^4.0.2",
    "@types/node": "^18.17.0",
    "@types/object-hash": "^3.0.3",
    "@types/randomstring": "^1.1.8",
    "@types/sort-array": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "browserify-cipher": "^1.0.1",
    "camel-case": "^4.1.2",
    "convert-units": "^2.3.4",
    "crypto-api-v1": "^1.0.0",
    "crypto-js": "^4.1.1",
    "eslint": "^8.47.0",
    "extra-promise": "^6.0.8",
    "fast-glob": "^3.3.1",
    "is-glob": "^4.0.3",
    "joi": "^17.9.2",
    "moment-timezone": "^0.5.43",
    "object-hash": "^3.0.0",
    "patrun": "^7.2.5",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "pupa": "^3.1.0",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "release-it": "^16.1.5",
    "shx": "^0.3.4",
    "sm-crypto-v2": "^1.7.0",
    "sort-array": "^4.1.5",
    "sort-keys": "^5.0.0",
    "terser": "^5.19.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "alex.ma",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lakutata/lakutata.git"
  },
  "homepage": "https://lakutata.com",
  "bugs": {
    "url": "https://github.com/lakutata/lakutata/issues",
    "email": "myq1991@gmail.com"
  },
  "keywords": [
    "lakutata",
    "framework",
    "typescript",
    "esm",
    "cjs",
    "ioc",
    "di"
  ]
}
