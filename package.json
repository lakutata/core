{
  "name": "lakutata",
  "version": "0.0.3",
  "description": "An IoC-based universal application framework.",
  "type": "module",
  "exports": {
    ".": {
      "require": "./build/Lakutata.cjs",
      "import": "./build/Lakutata.js",
      "types": "./build/Lakutata.d.ts"
    }
  },
  "main": "./build/Lakutata.cjs",
  "types": "./build/Lakutata.d.cts",
  "scripts": {
    "clean": "shx rm -rf ./build",
    "build": "tsup-node --config tsup.config.ts",
    "rebuild": "npm run clean && npm run build",
    "prepare": "npm run compile",
    "compile": "npm run rebuild && shx rm -rf ./build/tests/*",
    "release:major": "dotenv release-it major --git.tagExclude='*[-]*'",
    "release:major:beta": "dotenv release-it major --preRelease=beta",
    "release:major:alpha": "dotenv release-it major --preRelease=alpha",
    "release:minor": "dotenv release-it minor --git.tagExclude='*[-]*'",
    "release:minor:beta": "dotenv release-it minor --preRelease=beta",
    "release:minor:alpha": "dotenv release-it minor --preRelease=alpha",
    "release:patch": "dotenv release-it patch --git.tagExclude='*[-]*'",
    "release:patch:beta": "dotenv release-it patch --preRelease=beta",
    "release:patch:alpha": "dotenv release-it patch --preRelease=alpha",
    "release:preRelease": "dotenv  release-it --preRelease=alpha",
    "test": "ts-node src/tests/Test.spec.ts"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": false,
      "requireBranch": "main",
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_TOKEN",
      "autoGenerate": true
    }
  },
  "dependencies": {
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@types/convert-units": "^2.3.6",
    "@types/crypto-js": "^4.1.1",
    "@types/is-glob": "^4.0.2",
    "@types/node": "^18.17.0",
    "@types/object-hash": "^3.0.3",
    "@types/randomstring": "^1.1.8",
    "@types/sort-array": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "browserify-cipher": "^1.0.1",
    "camel-case": "^4.1.2",
    "convert-units": "^2.3.4",
    "crypto-api-v1": "^1.0.0",
    "crypto-js": "^4.1.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.47.0",
    "extra-promise": "^6.0.8",
    "fast-glob": "^3.3.1",
    "is-glob": "^4.0.3",
    "joi": "^17.9.2",
    "moment-timezone": "^0.5.43",
    "object-hash": "^3.0.0",
    "patrun": "^7.2.5",
    "pupa": "^3.1.0",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "release-it": "^16.1.5",
    "shx": "^0.3.4",
    "sm-crypto-v2": "^1.7.0",
    "sort-array": "^4.1.5",
    "sort-keys": "^5.0.0",
    "terser": "^5.19.2",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "alex.ma",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lakutata/lakutata.git"
  },
  "homepage": "https://lakutata.com",
  "bugs": {
    "url": "https://github.com/lakutata/lakutata/issues",
    "email": "myq1991@gmail.com"
  },
  "keywords": [
    "lakutata",
    "framework",
    "typescript",
    "esm",
    "cjs",
    "ioc",
    "di"
  ]
}
