{
  "name": "lakutata",
  "version": "0.1.2-proto.5",
  "description": "An IoC-based universal application framework.",
  "main": "./build/Lakutata.js",
  "types": "./build/Lakutata.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "shx rm -rf ./build",
    "build:cjs": "tsup-node --config tsup.config.cjs.ts",
    "build": "npm run build:cjs",
    "rebuild": "npm run clean && npm run build",
    "prepare": "npm run compile",
    "compile": "npm run rebuild",
    "prepublishOnly": "node .prepublish.js",
    "postpublish": "node .postpublish.js",
    "release": "dotenv -- release-it --git.tagExclude='*[-]*'",
    "release:beta-release": "dotenv -- release-it --preRelease=beta",
    "release:alpha-release": "dotenv -- release-it --preRelease=alpha --no-github.release",
    "release:proto-release": "dotenv -- release-it --preRelease=proto --no-github.release --no-git.tag",
    "test": "ts-node src/tests/Test.spec.ts"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": false,
      "requireBranch": false,
      "commitMessage": "chore: release v${version}",
      "push": false
    },
    "github": {
      "release": true,
      "autoGenerate": true
    }
  },
  "devDependencies": {
    "@types/convert-units": "^2.3.6",
    "@types/crypto-js": "^4.1.1",
    "@types/is-glob": "^4.0.2",
    "@types/node": "^18.17.0",
    "@types/node-schedule": "^2.1.0",
    "@types/object-hash": "^3.0.3",
    "@types/randomstring": "^1.1.8",
    "@types/sort-array": "^4.1.0",
    "@types/sync-fetch": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "axios": "^1.4.0",
    "browserify-cipher": "^1.0.1",
    "camel-case": "^4.1.2",
    "casbin": "^5.26.2",
    "casbin-file-adapter": "^1.0.1",
    "convert-units": "^2.3.4",
    "cron-validator": "^1.3.1",
    "crypto-js": "^4.1.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.47.0",
    "fast-glob": "^3.3.1",
    "html-tags": "^3.3.1",
    "is-glob": "^4.0.3",
    "is-plain-obj": "^4.1.0",
    "joi": "^17.9.2",
    "moment-timezone": "^0.5.43",
    "mongodb": "^5.8.1",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.13",
    "node-schedule": "^2.1.1",
    "object-hash": "^3.0.0",
    "patrun": "^7.2.5",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "piscina": "^4.1.0",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "release-it": "^16.1.5",
    "shx": "^0.3.4",
    "sm-crypto-v2": "^1.7.0",
    "sort-array": "^4.1.5",
    "sync-fetch": "^0.5.2",
    "terser": "^5.19.2",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typeorm": "^0.3.17",
    "typeorm-adapter": "^1.5.5",
    "typescript": "^5.1.6",
    "unionfs": "^4.5.1"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "alex.ma",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lakutata/lakutata.git"
  },
  "homepage": "https://lakutata.com",
  "bugs": {
    "url": "https://github.com/lakutata/lakutata/issues",
    "email": "myq1991@gmail.com"
  },
  "keywords": [
    "lakutata",
    "framework",
    "typescript",
    "alias",
    "cjs",
    "ioc",
    "di",
    "process",
    "thread",
    "worker",
    "daemon",
    "rbac"
  ]
}
