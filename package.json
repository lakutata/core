{
  "name": "@lakutata2/core",
  "version": "2.0.0",
  "description": "Lakutata Framework Core",
  "main": "./build/Core.cjs",
  "module": "./build/Core.js",
  "types": "./build/Core.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./build/Core.cjs",
      "import": "./build/Core.js",
      "types": "./build/Core.d.ts"
    }
  },
  "tsup": {
    "entry": [
      "src/**/*"
    ],
    "clean": true,
    "dts": {
      "resolve": true
    },
    "format": [
      "esm",
      "cjs"
    ],
    "outDir": "./build",
    "keepNames": true,
    "external": "pkgJson",
    "minify": "terser",
    "terserOptions": {
      "compress": true
    }
  },
  "scripts": {
    "clean": "shx rm -rf ./build",
    "build": "tsup",
    "rebuild": "npm run clean && npm run build",
    "prepare": "npm run compile",
    "compile": "npm run rebuild && shx rm -rf ./build/tests/*",
    "test": "ts-node src/tests/Test.spec.ts"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^18.17.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "joi": "^17.9.2",
    "pupa": "^3.1.0",
    "shx": "^0.3.4",
    "terser": "^5.19.2",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "alex.ma",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lakutata/core.git"
  },
  "homepage": "https://lakutata.com",
  "bugs": {
    "url": "https://github.com/lakutata/core/issues",
    "email": "myq1991@gmail.com"
  },
  "keywords": [
    "lakutata",
    "framework",
    "typescript"
  ]
}
