{
  "name": "@lakutata2/core",
  "version": "2.0.6",
  "description": "Lakutata Framework Core",
  "type": "module",
  "exports": {
    ".": {
      "require": "./build/Core.cjs",
      "import": "./build/Core.js",
      "types": "./build/Core.d.ts"
    },
    "./validator": {
      "require": "./build/Validator.cjs",
      "import": "./build/Validator.js",
      "types": "./build/Validator.d.ts"
    },
    "./decorator": {
      "require": "./build/Decorators.cjs",
      "import": "./build/Decorators.js",
      "types": "./build/Decorators.d.ts"
    },
    "./exception": {
      "require": "./build/Exceptions.cjs",
      "import": "./build/Exceptions.js",
      "types": "./build/Exceptions.d.ts"
    },
    "./utility": {
      "require": "./build/Utilities.cjs",
      "import": "./build/Utilities.js",
      "types": "./build/Utilities.d.ts"
    },
    "./formatter": {
      "require": "./build/Formatter.cjs",
      "import": "./build/Formatter.js",
      "types": "./build/Formatter.d.ts"
    },
    "./hash": {
      "require": "./build/Hash.cjs",
      "import": "./build/Hash.js",
      "types": "./build/Hash.d.ts"
    },
    "./crypto": {
      "require": "./build/Crypto.cjs",
      "import": "./build/Crypto.js",
      "types": "./build/Crypto.d.ts"
    }
  },
  "tsup": {
    "target": "es2017",
    "entry": [
      "src/**/*"
    ],
    "clean": true,
    "splitting": true,
    "dts": {
      "resolve": true
    },
    "format": [
      "esm",
      "cjs"
    ],
    "shims": true,
    "outDir": "./build",
    "keepNames": true,
    "external": "pkgJson",
    "minify": "terser",
    "terserOptions": {
      "compress": true
    }
  },
  "scripts": {
    "clean": "shx rm -rf ./build",
    "build": "tsup",
    "rebuild": "npm run clean && npm run build",
    "prepare": "npm run compile",
    "compile": "npm run rebuild && shx rm -rf ./build/tests/*",
    "test": "ts-node src/tests/Test.spec.ts"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.17.0",
    "@types/object-hash": "^3.0.2",
    "@types/randomstring": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "async-constructor": "^0.4.17",
    "browserify-cipher": "^1.0.1",
    "camel-case": "^4.1.2",
    "crypto-api-v1": "^1.0.0",
    "crypto-js": "^4.1.1",
    "eslint": "^8.45.0",
    "fast-glob": "^3.3.1",
    "is-glob": "^4.0.3",
    "joi": "^17.9.2",
    "moment-timezone": "^0.5.43",
    "object-hash": "^3.0.0",
    "pupa": "^3.1.0",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "shx": "^0.3.4",
    "sm-crypto-v2": "^1.7.0",
    "sort-array": "^4.1.5",
    "sort-keys": "^5.0.0",
    "terser": "^5.19.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "tsup": "^7.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "alex.ma",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lakutata/core.git"
  },
  "homepage": "https://lakutata.com",
  "bugs": {
    "url": "https://github.com/lakutata/core/issues",
    "email": "myq1991@gmail.com"
  },
  "keywords": [
    "lakutata",
    "framework",
    "typescript",
    "esm",
    "cjs"
  ]
}
