{
  "name": "lakutata",
  "version": "0.1.49-proto.6",
  "description": "An IoC-based universal application framework.",
  "main": "./Lakutata.js",
  "types": "./Lakutata.d.ts",
  "exports": {
    ".": {
      "import": "./Lakutata.js",
      "require": "./Lakutata.js",
      "types": "./Lakutata.d.ts"
    },
    "./ORM": {
      "import": "./ORM.js",
      "require": "./ORM.js",
      "types": "./ORM.d.ts"
    },
    "./Crypto": {
      "import": "./Crypto.js",
      "require": "./Crypto.js",
      "types": "./Crypto.d.ts"
    },
    "./Hash": {
      "import": "./Hash.js",
      "require": "./Hash.js",
      "types": "./Hash.d.ts"
    },
    "./Helper": {
      "import": "./Helper.js",
      "require": "./Helper.js",
      "types": "./Helper.d.ts"
    },
    "./ReflectMetadata": {
      "import": "./ReflectMetadata.js",
      "require": "./ReflectMetadata.js",
      "types": "./ReflectMetadata.d.ts"
    }
  },
  "bin": {
    "lakutata": "./CLI.js"
  },
  "scripts": {
    "clean": "shx rm -rf ./build",
    "build:cjs": "tsup-node --config tsup.config.cjs.ts",
    "build": "npm run build:cjs",
    "rebuild": "npm run clean && npm run build",
    "prepare": "npm run compile",
    "compile": "npm run rebuild",
    "publish": "npm run rebuild && node .publish.js",
    "release": "dotenv -- release-it --git.tagExclude='*[-]*' && npm run publish",
    "release:beta-release": "dotenv -- release-it --preRelease=beta && npm run publish",
    "release:alpha-release": "dotenv -- release-it --preRelease=alpha --no-github.release && npm run publish",
    "release:proto-release": "dotenv -- release-it --preRelease=proto --no-github.release --no-git.tag && npm run publish",
    "test": "ts-node src/tests/Example.spec.ts",
    "cli:link": "npm run rebuild && npm link",
    "cli:unlink": "npm r lakutata -g"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "requireCleanWorkingDir": true,
      "requireBranch": false,
      "commitMessage": "chore: release v${version}",
      "push": false
    },
    "github": {
      "release": true,
      "autoGenerate": true
    }
  },
  "devDependencies": {
    "@swc/core": "^1.3.92",
    "@types/convert-units": "^2.3.6",
    "@types/crypto-js": "^4.1.1",
    "@types/degit": "^2.8.3",
    "@types/find-root": "^1.1.2",
    "@types/inquirer": "^9.0.3",
    "@types/is-glob": "^4.0.2",
    "@types/node": "^18.17.0",
    "@types/node-schedule": "^2.1.0",
    "@types/object-hash": "^3.0.3",
    "@types/randomstring": "^1.1.8",
    "@types/semver": "^7.5.1",
    "@types/sort-array": "^4.1.0",
    "@types/sync-fetch": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "axios": "^1.4.0",
    "browserify-cipher": "^1.0.1",
    "camel-case": "^4.1.2",
    "casbin": "^5.26.2",
    "casbin-file-adapter": "^1.0.1",
    "chalk": "^5.3.0",
    "cli-spinners": "^2.9.0",
    "commander": "^11.0.0",
    "convert-units": "^2.3.4",
    "cron-validator": "^1.3.1",
    "crypto-js": "^4.1.1",
    "degit": "^2.8.4",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.47.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.1",
    "find-root": "^1.1.0",
    "html-tags": "^3.3.1",
    "inquirer": "^9.2.11",
    "is-glob": "^4.0.3",
    "is-plain-obj": "^4.1.0",
    "joi": "^17.9.2",
    "latest-version": "^7.0.0",
    "log-update": "^5.0.1",
    "moment-timezone": "^0.5.43",
    "node-fetch": "^2.6.13",
    "node-schedule": "^2.1.1",
    "object-hash": "^3.0.0",
    "patrun": "^7.2.5",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "piscina": "^4.1.0",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "release-it": "^16.1.5",
    "semver": "^7.5.4",
    "shx": "^0.3.4",
    "sm-crypto-v2": "^1.7.0",
    "sort-array": "^4.1.5",
    "sync-fetch": "^0.5.2",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typeorm": "0.3.14",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16"
  },
  "author": "alex.ma",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lakutata/lakutata.git"
  },
  "homepage": "https://lakutata.com",
  "bugs": {
    "url": "https://github.com/lakutata/lakutata/issues",
    "email": "admin@lakutata.com"
  },
  "keywords": [
    "lakutata",
    "framework",
    "typescript",
    "alias",
    "cjs",
    "ioc",
    "di",
    "process",
    "thread",
    "worker",
    "daemon",
    "rbac",
    "casbin",
    "typeorm"
  ]
}
